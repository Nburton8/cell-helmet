    "bc2c0ac7-9e32-4f40-ae2b-2afbfaa5dd82": {
      "type": "faq",
      "blocks": {
        "template--16192741212318__bc2c0ac7-9e32-4f40-ae2b-2afbfaa5dd82-16866769371ce03034-3": {
          "type": "category",
